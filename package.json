{
  "name": "@sjforge/feedback-widget-native",
  "version": "0.1.0",
  "description": "React Native feedback widget SDK with screenshots and context capture",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "react-native": "src/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --external react --external react-native --external @react-native-async-storage/async-storage --external @react-native-community/netinfo --external react-native-view-shot --external expo-device",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react-native",
    "feedback",
    "widget",
    "bug-report",
    "screenshot",
    "expo"
  ],
  "author": "SJ Forge",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sjforge/feedback-widget"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": ">=1.17.0",
    "@react-native-community/netinfo": ">=9.0.0",
    "expo-device": ">=5.0.0",
    "react": ">=17.0.0",
    "react-native": ">=0.70.0",
    "react-native-view-shot": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react-native-view-shot": {
      "optional": true
    },
    "expo-device": {
      "optional": true
    },
    "@react-native-community/netinfo": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/react": "^18.3.27",
    "@types/react-native": "^0.72.8",
    "eslint": "^9.0.0",
    "jest": "^29.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
